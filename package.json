{
  "name": "levelcompiler",
  "productName": "Level Compiler",
  "version": "0.1.0-beta.0",
  "description": "Stratigraphic Correlation Viewer and Compiler",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac --universal",
    "build": "electron-builder --win --mac"
  },
  "author": "kynpx npm-check-updates -u",
  "license": "MIT",
  "dependencies": {
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "electron-progressbar": "^2.2.1",
    "electron-prompt": "^1.7.0",
    "electron-updater": "^6.3.9",
    "jszip": "^3.10.1",
    "sharp": "^0.33.5"
  },
  "build": {
    "appId": "io.github.keitaroyamada.Level-Compiler",
    "publish": {
      "provider": "github",
      "owner": "keitaroyamada",
      "repo": "Level-Compiler"
    },
    "win": {
      "target": "msi",
      "icon": "./icon/levelcompiler.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "./icon/levelcompiler.png"
    },
    "linux": {
      "icon": "./icon/levelcompiler.png"
    },
    "extraResources": [
      "resources/"
    ]
  },
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "electronmon": "^2.0.3"
  }
}
